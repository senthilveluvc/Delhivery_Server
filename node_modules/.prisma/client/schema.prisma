generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Warehouse {
  warehouse_name String  @id
  uuid           String  @unique @default(uuid()) @db.Uuid
  email          String?
  phone          String?
  address        String?
  city           String?
  country        String?
  pincode        String?
}

model Customer_shipment {
  orderID       String  @id // Primary Key
  uuid          String  @unique @default(uuid()) @db.Uuid
  customer_name String?
  address       String?
  phone         String?
  pincode       String?
  payment_mode  String  @default("Prepaid")
  shipment_mode String?
  width         Float?
  height        Float?
  weight        Float?
  product_desc  String?
  business_name String?
  waybill       String?
  status        String?
  order_json    Json?
}
